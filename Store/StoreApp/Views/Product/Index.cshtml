@model List<StoreApp.Models.Product>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    @foreach (var product in Model)
    {
        <h3>@product.Name</h3>
        <p>@product.Price</p>
    }
</body>
</html>